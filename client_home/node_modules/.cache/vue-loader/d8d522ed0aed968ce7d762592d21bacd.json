v class=\"container\">\n\t\t<div class=\"row\">\n\t\t  <div class=\"col-12\">\n\t\t\t<div class=\"card_result_search\">\n\t\t\t  <div class=\"title\">搜索结果</div>\n\t\t\t\t<!-- 商品搜索结果 -->\n\t\t\t  <list_result_search\n\t\t\t\t:list=\"result_goods\"\n\t\t\t\ttitle=\"图书商城\"\n\t\t\t\tsource_table=\"goods\"\n\t\t\t  ></list_result_search>\n\t\t\t\t<!-- 文章搜索结果 -->\n\t\t\t  <list_result_search\n\t\t\t\t:list=\"result_article\"\n\t\t\t\ttitle=\"店铺活动\"\n\t\t\t\tsource_table=\"article\"\n\t\t\t  ></list_result_search>\n\t\t\t\t<!-- 论坛搜索结果 -->\n\t\t\t  <list_result_search\n\t\t\t\t:list=\"result_forum\"\n\t\t\t\ttitle=\"留言交流\"\n\t\t\t\tsource_table=\"forum\"\n\t\t\t  ></list_result_search>\n\t\t\t  <list_result_search\n\t\t\t\tv-if=\"$check_action('/registered_user/list', 'get')\"\n\t\t\t\t:list=\"result_registered_user_user_name\"\n\t\t\t\ttitle=\"注册用户用户名\"\n\t\t\t\tsource_table=\"registered_user\"\n\t\t\t  ></list_result_search>\n\t\t\t  <list_result_search\n\t\t\t\tv-if=\"$check_action('/book_mall/list', 'get')\"\n\t\t\t\t:list=\"result_book_mall_author\"\n\t\t\t\ttitle=\"图书商城作者\"\n\t\t\t\tsource_table=\"book_mall\"\n\t\t\t  ></list_result_search>\n\t\t\t  <list_result_search\n\t\t\t\tv-if=\"$check_action('/book_mall/list', 'get')\"\n\t\t\t\t:list=\"result_book_mall_press\"\n\t\t\t\ttitle=\"图书商城出版社\"\n\t\t\t\tsource_table=\"book_mall\"\n\t\t\t  ></list_result_search>\n\t\t\t  <list_result_search\n\t\t\t\tv-if=\"$check_action('/shopowner/list', 'get')\"\n\t\t\t\t:list=\"result_shopowner_store_manager_job_number\"\n\t\t\t\ttitle=\"店长店长工号\"\n\t\t\t\tsource_table=\"shopowner\"\n\t\t\t  ></list_result_search>\n\t\t\t</div>\n\t\t  </div>\n\t\t</div>\n\t  </div>\n\t</div>\n  </div>\n</template>\n\n<script>\nimport mixin from \"../../mixins/page.js\";\nimport list_result_search from \"../../components/diy/list_result_search.vue\";\n\nexport default {\n  mixins: [mixin],\n  data() {\n\treturn {\n\t  \"query\": {\n\t\tword: \"\",\n\t  },\n\t  \"result_goods\": [],\n\t  \"result_article\": [],\n\t  \"result_forum\": [],\n\t\t\t\"result_registered_user_user_name\":[],\n\t\t\t\"result_book_mall_author\":[],\n\t\t\t\"result_book_mall_press\":[],\n\t\t\t\"result_shopowner_store_manager_job_number\":[],\n\t};\n  },\n  methods: {\n\t/**\n\t * 获取图书商城\n\t */\n\tget_goods() {\n\t  this.$get(\"~/api/goods/get_list?like=0\", { page: 1, size: 10, title: this.query.word }, (json) => {\n\t\tif (json.result) {\n\t\t  this.result_goods = json.result.list;\n\t\t}\n\t  });\n\t},\n\t/**\n\t * 获取文章\n\t */\n\tget_article() {\n\t  this.$get(\"~/api/article/get_list?like=0\", { page: 1, size: 10, title: this.query.word }, (json) => {\n\t\tif (json.result) {\n\t\t  this.result_article = json.result.list;\n\t\t}\n\t  });\n\t},\n\t/**\n\t * 获取留言交流\n\t */\n\tget_forum() {\n\t  this.$get(\"~/api/forum/get_list?like=0\", { page: 1, size: 10, title: this.query.word }, (json) => {\n\t\tif (json.result) {\n\t\t  this.result_forum = json.result.list;\n\t\t}\n\t  });\n\t},\n\t/**\n\t * 获取user_name\n\t */\n\tget_registered_user_user_name(){\n\t\tthis.$get(\"~/api/registered_user/get_list?like=0\", { page: 1, size: 10, \"user_name\": this.query.word }, (json) => {\n\t\t  if (json.result) {\n\t\t\tvar result_registered_user_user_name = json.result.list;\n\t\t\tresult_registered_user_user_name.map(o => o.title = o['user_name'])\n\t  \t\t\tthis.result_registered_user_user_name = result_registered_user_user_name\n\t\t \t}\n\t\t});\n\t},\n\t/**\n\t * 获取author\n\t */\n\tget_book_mall_author(){\n\t\tthis.$get(\"~/api/book_mall/get_list?like=0\", { page: 1, size: 10, \"author\": this.query.word }, (json) => {\n\t\t  if (json.result) {\n\t\t\tvar result_book_mall_author = json.result.list;\n\t\t\tresult_book_mall_author.map(o => o.title = o['author'])\n\t  \t\t\tthis.result_book_mall_author = result_book_mall_author\n\t\t \t}\n\t\t});\n\t},\n\t/**\n\t * 获取press\n\t */\n\tget_book_mall_press(){\n\t\tthis.$get(\"~/api/book_mall/get_list?like=0\", { page: 1, size: 10, \"press\": this.query.word }, (json) => {\n\t\t  if (json.result) {\n\t\t\tvar result_book_mall_press = json.result.list;\n\t\t\tresult_book_mall_press.map(o => o.title = o['press'])\n\t  \t\t\tthis.result_book_mall_press = result_book_mall_press\n\t\t \t}\n\t\t});\n\t},\n\t/**\n\t * 获取store_manager_job_number\n\t */\n\tget_shopowner_store_manager_job_number(){\n\t\tthis.$get(\"~/api/shopowner/get_list?like=0\", { page: 1, size: 10, \"store_manager_job_number\": this.query.word }, (json) => {\n\t\t  if (json.result) {\n\t\t\tvar result_shopowner_store_manager_job_number = json.result.list;\n\t\t\tresult_shopowner_store_manager_job_number.map(o => o.title = o['store_manager_job_number'])\n\t  \t\t\tthis.result_shopowner_store_manager_job_number = result_shopowner_store_manager_job_number\n\t\t \t}\n\t\t});\n\t},\n\n  },\n  components: { list_result_search },\n\tcreated(){\n    this.query.word = this.$route.query.word || \"\";\n  },\n  mounted() {\n\tthis.get_goods();\n\tthis.get_article();\n\tthis.get_forum();\n\t\tthis.get_registered_user_user_name();\n\t\tthis.get_book_mall_author();\n\t\tthis.get_book_mall_press();\n\t\tthis.get_shopowner_store_manager_job_number();\n  },\n  watch: {\n\t$route() {\n\t  $.push(this.query, this.$route.query);\n\t  this.get_goods();\n\t  this.get_article();\n\t  this.get_forum();\n\t  this.get_registered_user_user_name();\n\t  this.get_book_mall_author();\n\t  this.get_book_mall_press();\n\t  this.get_shopowner_store_manager_job_number();\n\t},\n  },\n};\n</script>\n\n<style scoped>\n.card_search {\n  text-align: center;\n}\n.card_result_search>.title {\n  text-align: center;\n  padding: 10px 0;\n}\n</style>\n"]}]}