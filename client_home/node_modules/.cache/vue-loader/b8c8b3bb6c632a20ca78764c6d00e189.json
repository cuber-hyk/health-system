"sourcesContent":["<template>\n  <div class=\"list_collect\">\n    <div class=\"item_collect\" v-for=\"(o, i) in list\" :key=\"i\">\n      <router-link\n        class=\"collect_box\"\n        :to=\"\n          '/' +\n          o[vm.source_table] +\n          '/details?' +\n          o[vm.source_field] +\n          '=' +\n          o[vm.source_id]\n        \"\n      >\n        <div class=\"left_block\">\n          <img\n            style=\"width: 3rem; height: 3rem\"\n            :src=\"o[vm.img] ? $fullUrl(o[vm.img]) : '/img/default.png'\"\n          />\n        </div>\n        <div class=\"right_block\">\n          <div class=\"top_comment\">\n            <div class=\"title\">\n              {{ o[vm.title] }}\n            </div>\n            <div class=\"time\">\n              {{ o[vm.create_time] | formatDate}}\n            </div>\n          </div>\n        </div>\n      </router-link>\n      <b-icon class=\"btn_delete\" icon=\"trash\" @click=\"del_collect(o[vm.collect_id], i)\"></b-icon>\n    </div>\n  </div>\n</template>\n\n<script>\nimport mixin from \"@/mixins/page.js\";\nexport default {\n  mixins: [mixin],\n  props: {\n    list: {\n      type: Array,\n      default: function () {\n        return [];\n      },\n    },\n    vm: {\n      type: Object,\n      default: function () {\n        return {\n          img: \"img\",\n          title: \"title\",\n          create_time: \"create_time\",\n          collect_id: \"collect_id\",\n          source_table: \"source_table\",\n          source_field: \"source_field\",\n          source_id: \"source_id\",\n        };\n      },\n    },\n  },\n  methods: {\n    /**\n     * 删除收藏\n     * @param { Number } collect_id 收藏id\n     * @param { Number } index 删除位置\n      */\n    del_collect(collect_id, index) {\n      this.$get(\n        \"~/api/collect/del\",\n        {\n          collect_id,\n        },\n        (res) => {\n          this.list.splice(index, 1);\n        }\n      );\n    },\n  },\n};\n</script>\n\n<style scoped>\n.item_collect {\n  position: relative;\n  background-color: #ffffff;\n  margin-bottom: 0.8rem;\n}\n\n.collect_box {\n  display: flex;\n  padding: 0.3rem 0.6rem;\n}\n\n.left_block {\n  display: inline-block;\n  width: 20%;\n  padding: 0.3rem;\n  padding-right: 0.5rem;\n}\n\n.right_block {\n  display: inline-block;\n  width: 80%;\n}\n\n.time {\n  color: #999;\n}\n\n.top_comment {\n  display: flex;\n  justify-content: space-between;\n  margin-bottom: 0.6rem;\n}\n.top_comment .title {\n  flex: 0 0 55%;\n  overflow: hidden;\n  text-overflow: ellipsis;\n  white-space: nowrap;\n}\n.top_comment .time {\n  flex: 20%;\n  font-size: 0.5rem;\n}\n\n.btn_delete {\n  position: absolute;\n  right: 0.7rem;\n  bottom: 0.8rem;\n  line-height: 20px;\n  font-size: 15px;\n}\n.btn_delete:hover {\n  background: var(--color_primary_b);\n  border-color: var(--color_primary_b);\n}\n</style>\n"]}]}