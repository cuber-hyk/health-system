{"remainingRequest":"C:\\Users\\胡运宽\\Desktop\\springboot-new\\client_home\\node_modules\\vue-loader\\lib\\index.js??vue-loader-options!C:\\Users\\胡运宽\\Desktop\\springboot-new\\client_home\\src\\views\\index.vue?vue&type=style&index=0&id=395094c2&prod&scoped=true&lang=css&","dependencies":[{"path":"C:\\Users\\胡运宽\\Desktop\\springboot-new\\client_home\\src\\views\\index.vue","mtime":1729768901222},{"path":"C:\\Users\\胡运宽\\Desktop\\springboot-new\\client_home\\node_modules\\css-loader\\dist\\cjs.js","mtime":499162500000},{"path":"C:\\Users\\胡运宽\\Desktop\\springboot-new\\client_home\\node_modules\\vue-loader\\lib\\loaders\\stylePostLoader.js","mtime":499162500000},{"path":"C:\\Users\\胡运宽\\Desktop\\springboot-new\\client_home\\node_modules\\postcss-loader\\src\\index.js","mtime":499162500000},{"path":"C:\\Users\\胡运宽\\Desktop\\springboot-new\\client_home\\node_modules\\cache-loader\\dist\\cjs.js","mtime":499162500000},{"path":"C:\\Users\\胡运宽\\Desktop\\springboot-new\\client_home\\node_modules\\vue-loader\\lib\\index.js","mtime":499162500000}],"contextDependencies":[],"result":[{"type":"Buffer","data":"base64:"},{"version":3,"sources":["index.vue"],"names":[],"mappings":";AAotBA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA","file":"index.vue","sourceRoot":"src/views","sourcesContent":["<template>\n  <div class=\"page_root\" id=\"root_demo\">\n    <div class=\"warp\">\n      <div class=\"container\">\n        <div class=\"row\">\n          <div class=\"col\">\n            <div class=\"card_ad\">\n              <!-- 页头 -->\n              <list_ad\n                :list=\"list_ad\"\n                location=\"店招\"\n                v-if=\"$check_action('/ad/list', 'get')\"\n                style=\"marginbottom: 0.5rem\"\n              />\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n\n    <div class=\"warp\">\n      <div class=\"container swiper_box\">\n        <div class=\"row\">\n          <div class=\"col-12\">\n            <div class=\"sw_no\">\n              <!-- 轮播图组件 -->\n              <div class=\"swiper_img\">\n                <swiper_img :list=\"list_slide\" />\n              </div>\n              <div class=\"card_notice\">\n                <div class=\"notice_title\">\n                  公告信息 <router-link to=\"notice/list\">More</router-link>\n                </div>\n                <!-- 公告组件 -->\n                <swiper_notice :list=\"list_notice\" />\n              </div>\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n\n    <div class=\"warp\">\n      <div class=\"container\">\n        <div class=\"row\">\n          <div class=\"col\">\n            <div class=\"card_ad\">\n              <!-- 顶部 -->\n              <list_ad\n                :list=\"list_ad\"\n                v-if=\"$check_action('/ad/list', 'get')\"\n                location=\"顶部广告\"\n              />\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n\n    <div class=\"warp\">\n      <div class=\"container\">\n        <div class=\"row\">\n          <div class=\"col\">\n            <div class=\"card_ad\">\n              <!-- 中部 -->\n              <list_ad\n                :list=\"list_ad\"\n                v-if=\"$check_action('/ad/list', 'get')\"\n                location=\"中部广告\"\n              />\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n\n    <div class=\"warp\">\n      <!-- 容器 -->\n      <div class=\"container\">\n        <div class=\"row\">\n          <div class=\"col-12\">\n            <!-- 标题栏组件 -->\n            <div class=\"article_recommend\">\n              <div class=\"title\">\n                <span> 健康百科 </span>\n              </div>\n              <div class=\"more_box\">\n                <router-link to=\"/article/list\" class=\"more\">\n                  <span>More</span>\n                </router-link>\n              </div>\n            </div>\n            <!-- 健康百科列表组件 -->\n            <list_article v-if=\"show_list_article\" :list=\"list_article\" />\n            <div class=\"overflow-auto\" v-else>\n              <table\n                id=\"list_article\"\n                role=\"table\"\n                aria-busy=\"false\"\n                :aria-colcount=\"fields_article.length\"\n                class=\"table b-table table-striped table-hover\"\n              >\n                <thead>\n                  <tr>\n                    <th v-for=\"(o, i) in fields_article\" :key=\"i\">\n                      {{ o.label }}\n                    </th>\n                  </tr>\n                </thead>\n                <tbody>\n                  <tr v-for=\"(o, i) in list_table_article\" :key=\"i\">\n                    <td\n                      v-for=\"(oj, n) in fields_goods\"\n                      @click=\"to_details('goods', o)\"\n                    >\n                      <img\n                        v-if=\"oj.type && oj.type == '图片'\"\n                        :src=\"$fullUrl(o[oj.key])\"\n                        alt=\"\"\n                        v-default-img=\"'/img/default.png'\"\n                      />\n                      <span v-else>{{ o[oj.key] }}</span>\n                    </td>\n                  </tr>\n                </tbody>\n              </table>\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n\n    <div class=\"warp\">\n      <div class=\"container\">\n        <div class=\"row\">\n          <div class=\"col\">\n            <div class=\"card_ad\">\n              <!-- 底部 -->\n              <list_ad\n                :list=\"list_ad\"\n                v-if=\"$check_action('/ad/list', 'get')\"\n                location=\"底部广告\"\n              />\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n\n    <!-- 推荐食物信息模块(开始) -->\n    <div class=\"warp\" v-if=\"$check_action('/food_information/list', 'get')\">\n      <!-- 容器 -->\n      <div class=\"container\">\n        <div class=\"row\">\n          <div class=\"col-12\">\n            <!-- 标题栏组件 -->\n            <div class=\"diy_recommend\">\n              <div class=\"title\">\n                <span> 食物信息推荐 </span>\n              </div>\n              <div class=\"more_box\">\n                <router-link to=\"/food_information/list\" class=\"more\">\n                  <span>More</span>\n                </router-link>\n              </div>\n            </div>\n            <!-- 食物信息推荐列表组件 -->\n            <list_food_information\n              v-if=\"show_list_food_information\"\n              :list=\"list_food_information\"\n            />\n            <div class=\"overflow-auto\" v-else>\n              <table\n                id=\"list_diy\"\n                role=\"table\"\n                aria-busy=\"false\"\n                :aria-colcount=\"fields_food_information.length\"\n                class=\"table b-table table-striped table-hover\"\n              >\n                <thead>\n                  <tr>\n                    <th v-for=\"(o, i) in fields_food_information\" :key=\"i\">\n                      {{ o.label }}\n                    </th>\n                  </tr>\n                </thead>\n                <tbody>\n                  <tr\n                    v-for=\"(o, i) in list_table_food_information\"\n                    :key=\"i\"\n                    @click=\"\n                      to_details('food_information', o, 'food_information_id')\n                    \"\n                  >\n                    <td v-for=\"(oj, n) in fields_food_information\">\n                      <img\n                        v-if=\"oj.type && oj.type == '图片'\"\n                        :src=\"$fullUrl(o[oj.key])\"\n                        alt=\"\"\n                        v-default-img=\"'/img/default.png'\"\n                      />\n                      <span v-else-if=\"oj.type && oj.type == 'UID'\">\n                        <span style=\"display: none\">\n                          {{ get_user_by_user_id(o, oj, oj.key) }}\n                        </span>\n                        {{ oj.value }}\n                      </span>\n                      <span v-else>{{ o[oj.key] }}</span>\n                    </td>\n                  </tr>\n                </tbody>\n              </table>\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n    <!-- 推荐食物信息模块(结束) -->\n    <!-- 推荐食谱信息模块(开始) -->\n    <div class=\"warp\" v-if=\"$check_action('/recipe_information/list', 'get')\">\n      <!-- 容器 -->\n      <div class=\"container\">\n        <div class=\"row\">\n          <div class=\"col-12\">\n            <!-- 标题栏组件 -->\n            <div class=\"diy_recommend\">\n              <div class=\"title\">\n                <span> 食谱信息推荐 </span>\n              </div>\n              <div class=\"more_box\">\n                <router-link to=\"/recipe_information/list\" class=\"more\">\n                  <span>More</span>\n                </router-link>\n              </div>\n            </div>\n            <!-- 食谱信息推荐列表组件 -->\n            <list_recipe_information\n              v-if=\"show_list_recipe_information\"\n              :list=\"list_recipe_information\"\n            />\n            <div class=\"overflow-auto\" v-else>\n              <table\n                id=\"list_diy\"\n                role=\"table\"\n                aria-busy=\"false\"\n                :aria-colcount=\"fields_recipe_information.length\"\n                class=\"table b-table table-striped table-hover\"\n              >\n                <thead>\n                  <tr>\n                    <th v-for=\"(o, i) in fields_recipe_information\" :key=\"i\">\n                      {{ o.label }}\n                    </th>\n                  </tr>\n                </thead>\n                <tbody>\n                  <tr\n                    v-for=\"(o, i) in list_table_recipe_information\"\n                    :key=\"i\"\n                    @click=\"\n                      to_details(\n                        'recipe_information',\n                        o,\n                        'recipe_information_id'\n                      )\n                    \"\n                  >\n                    <td v-for=\"(oj, n) in fields_recipe_information\">\n                      <img\n                        v-if=\"oj.type && oj.type == '图片'\"\n                        :src=\"$fullUrl(o[oj.key])\"\n                        alt=\"\"\n                        v-default-img=\"'/img/default.png'\"\n                      />\n                      <span v-else-if=\"oj.type && oj.type == 'UID'\">\n                        <span style=\"display: none\">\n                          {{ get_user_by_user_id(o, oj, oj.key) }}\n                        </span>\n                        {{ oj.value }}\n                      </span>\n                      <span v-else>{{ o[oj.key] }}</span>\n                    </td>\n                  </tr>\n                </tbody>\n              </table>\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n    <!-- 推荐食谱信息模块(结束) -->\n\n    <div class=\"warp\">\n      <div class=\"container\">\n        <div class=\"row\">\n          <div class=\"col\">\n            <div class=\"card_link\">\n              <!-- 底部友情链接 -->\n              <list_link\n                v-if=\"$check_action('/link/list', 'get')\"\n                :list=\"list_link\"\n              />\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport mixin from \"@/mixins/page.js\";\nimport list_food_information from \"@/components/diy/list_food_information.vue\";\nimport list_recipe_information from \"@/components/diy/list_recipe_information.vue\";\nimport bar_title from \"@/components/diy/bar_title.vue\";\nimport list_article from \"@/components/diy/list_article.vue\";\nimport swiper_img from \"@/components/diy/swiper_img.vue\";\nimport list_ad from \"@/components/diy/list_ad.vue\";\nimport list_link from \"@/components/diy/list_link.vue\";\nimport swiper_notice from \"@/components/diy/swiper_notice.vue\";\n\nexport default {\n  mixins: [mixin],\n  components: {\n    list_food_information,\n    list_recipe_information,\n    bar_title,\n    list_article,\n    swiper_img,\n    swiper_notice,\n    list_ad,\n    list_link,\n  },\n  data() {\n    return {\n      isSmall: false,\n      sendValue: \"\",\n      chatList: [],\n      showChat: false,\n      isAdmin: false,\n      token: \"\",\n      list_type_commodity_type: [],\n      // 文章模型数组\n      list_article: [],\n      vm_arr_article: [\n        \"img\",\n        \"title\",\n        \"description\",\n        \"hits\",\n        \"type\",\n        \"tag\",\n        \"source\",\n        \"url\",\n        \"create_time\",\n      ],\n      fields_article: [\n        { key: \"img\", label: \"图片\" },\n        { key: \"title\", label: \"标题\" },\n        { key: \"description\", label: \"描述\" },\n        { key: \"hits\", label: \"点击量\" },\n        { key: \"type\", label: \"分类\" },\n        { key: \"tag\", label: \"标签\" },\n        { key: \"source\", label: \"来源\" },\n        { key: \"url\", label: \"来源地址\" },\n        { key: \"create_time\", label: \"发布时间\" },\n      ],\n      show_list_article: true,\n      show_list_food_information: true,\n      list_food_information: [],\n      vm_arr_food_information: [\n        \"food_information_id\",\n        \"food_name\",\n        \"food_classification\",\n        \"cover\",\n        \"taboo\",\n        \"energy\",\n        \"intended_for\",\n        \"food_introduction\",\n      ],\n      fields_food_information: [\n        {\n          key: \"food_name\",\n          label: \"食物名称\",\n          type: \"文本\",\n        },\n        {\n          key: \"food_classification\",\n          label: \"食物分类\",\n          type: \"下寻\",\n        },\n        {\n          key: \"cover\",\n          label: \"封面\",\n          type: \"图片\",\n        },\n        {\n          key: \"taboo\",\n          label: \"食用禁忌\",\n          type: \"文本\",\n        },\n        {\n          key: \"energy\",\n          label: \"能量\",\n          type: \"文本\",\n        },\n        {\n          key: \"intended_for\",\n          label: \"适用人群\",\n          type: \"文本\",\n        },\n        {\n          key: \"food_introduction\",\n          label: \"食物介绍\",\n          type: \"编辑\",\n        },\n      ],\n      show_list_recipe_information: true,\n      list_recipe_information: [],\n      vm_arr_recipe_information: [\n        \"recipe_information_id\",\n        \"recipe_name\",\n        \"recipe_classification\",\n        \"cover\",\n        \"taboo\",\n        \"energy\",\n        \"intended_for\",\n        \"make_video\",\n        \"recipe_introduction\",\n      ],\n      fields_recipe_information: [\n        {\n          key: \"recipe_name\",\n          label: \"食谱名称\",\n          type: \"文本\",\n        },\n        {\n          key: \"recipe_classification\",\n          label: \"食谱分类\",\n          type: \"下寻\",\n        },\n        {\n          key: \"cover\",\n          label: \"封面\",\n          type: \"图片\",\n        },\n        {\n          key: \"taboo\",\n          label: \"食用禁忌\",\n          type: \"文本\",\n        },\n        {\n          key: \"energy\",\n          label: \"能量\",\n          type: \"文本\",\n        },\n        {\n          key: \"intended_for\",\n          label: \"适用人群\",\n          type: \"文本\",\n        },\n        {\n          key: \"make_video\",\n          label: \"制作视频\",\n          type: \"视频\",\n        },\n        {\n          key: \"recipe_introduction\",\n          label: \"食谱介绍\",\n          type: \"编辑\",\n        },\n      ],\n      list_slide: [],\n      list_menu: [],\n      list_link: [],\n      list_notice: [],\n      list_ad: [],\n    };\n  },\n  created() {\n    this.get_token();\n    this.get_list_type_commodity_type();\n  },\n  methods: {\n    get_token() {\n      let token = this.$route.query.token;\n      if (token) {\n        $.db.set(\"token\", token, 120);\n        location.href = \"http://60.204.223.60:5002/\";\n      }\n    },\n    // toggle\n    toToggle() {\n      this.isAdmin = !this.isAdmin;\n    },\n\n    // 获取食物信息列表\n    get_food_information() {\n      let url = \"~/api/food_information/get_list?\";\n      this.$get(\n        url,\n        {\n          page: 1,\n          size: 8,\n        },\n        (json) => {\n          if (json.result) {\n            this.list_food_information = json.result.list;\n          }\n        }\n      );\n    },\n    // 获取食谱信息列表\n    get_recipe_information() {\n      let url = \"~/api/recipe_information/get_list?\";\n      url = \"~/api/recipe_information/get_hits_list?\";\n      this.$get(\n        url,\n        {\n          user_id: this.$store.state.user.user_id,\n          page: 1,\n          size: 8,\n        },\n        (json) => {\n          if (json.result) {\n            this.list_recipe_information = json.result.list;\n          }\n        }\n      );\n    },\n\n    /**\n     * 当前年月日时分秒方法\n     * @param {Object} fmt\n     */\n    dateFormat(fmt) {\n      var myDate = new Date();\n      var o = {\n        \"M+\": myDate.getMonth() + 1, // 月份\n        \"d+\": myDate.getDate(), // 日\n        \"h+\": myDate.getHours(), // 小时\n        \"m+\": myDate.getMinutes(), // 分\n        \"s+\": myDate.getSeconds(), // 秒\n        \"q+\": Math.floor((myDate.getMonth() + 3) / 3), // 季度\n        S: myDate.getMilliseconds(), // 毫秒\n      };\n      if (/(y+)/.test(fmt))\n        fmt = fmt.replace(\n          RegExp.$1,\n          (myDate.getFullYear() + \"\").substr(4 - RegExp.$1.length)\n        );\n      for (var k in o)\n        if (new RegExp(\"(\" + k + \")\").test(fmt))\n          fmt = fmt.replace(\n            RegExp.$1,\n            RegExp.$1.length == 1\n              ? o[k]\n              : (\"00\" + o[k]).substr((\"\" + o[k]).length)\n          );\n      return fmt;\n    },\n\n    // 获取轮播图\n    get_slides() {\n      this.$get(\"~/api/slides/get_list?\", {}, (json) => {\n        if (json.result) {\n          this.list_slide = json.result.list;\n        }\n      });\n    },\n    // 获取导航栏\n    get_menu() {\n      var user_group = this.user_group;\n      this.$get(\n        \"~/api/auth/get_list?\",\n        {\n          page: \"\",\n          user_group,\n          get: 1,\n          position: \"top\",\n        },\n        (json) => {\n          if (json.result) {\n            this.list_menu = json.result.list;\n          }\n        }\n      );\n    },\n    // 获取文章\n    get_article() {\n      this.$get(\n        \"~/api/article/get_list?\",\n        {\n          page: 1,\n          size: 4,\n        },\n        (json) => {\n          if (json.result) {\n            var list_article = json.result.list;\n            this.list_article = list_article;\n          }\n        }\n      );\n    },\n    // 获取广告\n    get_ad() {\n      this.$get(\n        \"~/api/ad/get_list?\",\n        {\n          orderby: \"`display` desc\",\n        },\n        (json) => {\n          if (json.result) {\n            var list_ad = json.result.list;\n            this.list_ad = list_ad;\n          }\n        }\n      );\n    },\n    // 获取链接列表\n    get_link() {\n      this.$get(\n        \"~/api/link/get_list?\",\n        {\n          page: 1,\n          size: 8,\n        },\n        (json) => {\n          if (json.result) {\n            var list_link = json.result.list;\n            this.list_link = list_link;\n          }\n        }\n      );\n    },\n    // 获取公告列表\n    get_notice() {\n      this.$get(\n        \"~/api/notice/get_list?\",\n        {\n          page: 1,\n          size: 3,\n        },\n        (json) => {\n          if (json.result) {\n            var list_notice = json.result.list;\n            this.list_notice = list_notice;\n          }\n        }\n      );\n    },\n    to_details(key, o, id) {\n      if (!id) {\n        id = key + \"_id\";\n      }\n      this.$router.push(\"/\" + key + \"/details?\" + id + \"=\" + o[id]);\n    },\n    async get_list_type_commodity_type() {\n      var json = await this.$get(\n        \"~/api/custom_goods/get_list?groupby=commodity_type\"\n      );\n      if (json.result) {\n        this.list_type_commodity_type = json.result.list;\n        console.log(this.list_type_commodity_type);\n        this.query.commodity_type =\n          this.list_type_commodity_type[0].commodity_type;\n        this.get_list();\n  