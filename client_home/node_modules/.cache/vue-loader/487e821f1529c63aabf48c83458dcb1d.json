\t\t\t\t\t\t\t\t\t</b-dropdown-item>\n\t\t\t\t\t\t\t\t</b-dropdown>\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t</div>\n\t\t\t\t\t\t\t<!-- /筛选 -->\n\t\t\t\t\t\t</div>\n\t\t\t\t\t</div>\n\t\t\t\t\t<div class=\"diy_list_sort_box\">\n\t\t\t\t\t\t<div class=\"col\">\n\t\t\t\t\t\t\t<!-- 排序 -->\n\t\t\t\t\t\t\t<div class=\"view\">\n\t\t\t\t\t\t\t\t<b-dropdown text=\"排序\" variant=\"outline-dark\" left>\n\t\t\t\t\t\t\t\t\t\t<b-dropdown-item v-for=\"(o, i) in list_sort\" :key=\"i\" @click=\"set_sort(o)\" >\n\t\t\t\t\t\t\t\t\t\t\t\t{{ o.name }}\n\t\t\t\t\t\t\t\t\t\t</b-dropdown-item>\n\t\t\t\t\t\t\t\t</b-dropdown>\n\t\t\t\t\t\t\t</div>\n\t\t\t\t\t\t\t<!--/排序 -->\n\t\t\t\t\t\t</div>\n\t\t\t\t\t</div>\n\t\t\t\t</div>\n\n\t\t\t\t<div class=\"row diy_list_box\">\n\t\t\t\t\t<div class=\"col\">\n\t\t\t\t\t\t<!-- tab -->\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t<b-tabs content-class=\"mt-3\">\n\t\t\t\t\t\t\t<b-tab v-for=\"(o, i) in list_type_book_category\" :title=\"o['book_category']\" :active=\"i===0\" @click=\"tab_select(o['book_category'])\">\n\t\t\t\t\t\t\t\t<div class=\"row\">\n\t\t\t\t\t\t\t\t\t<div class=\"col\">\n\t\t\t\t\t\t\t\t\t\t<!-- tab -->\n\t\t\t\t\t\t\t\t\t\t<list_book_information :list=\"type_list[o['book_category']]\" />\n\t\t\t\t\t\t\t\t\t\t<!-- /tab -->\n\t\t\t\t\t\t\t\t\t</div>\n\t\t\t\t\t\t\t\t</div>\n\t\t\t\t\t\t\t</b-tab>\n\t\t\t\t\t\t</b-tabs>\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t<!-- /tab -->\n\t\t\t\t\t</div>\n\t\t\t\t</div>\n\t\t\t\t<div class=\"row diy_list_page_box\">\n\t\t\t\t\t<div class=\"col overflow-auto flex_cc\">\n\t\t\t\t\t\t<!-- 分页器 -->\n<!--\t\t\t\t\t\t<diy_pager v-model=\"query['page']\" :size=\"query['size']\" :count=\"count\" v-on:toPage=\"toPage\" v-on:toSize=\"toSize\" ></diy_pager>-->\n            <b-pagination\n                v-model=\"query.page\"\n                :total-rows=\"count\"\n                :per-page=\"query.size\"\n                @change=\"goToPage\"\n            />\n\t\t\t\t\t\t<!-- /分页器 -->\n\t\t\t\t\t</div>\n\t\t\t\t</div>\n\t\t\t</div>\n\t\t</div>\n\t</div>\n</template>\n\n<script>\n\timport list_book_information from \"@/components/diy/list_book_information.vue\";\n\timport diy_pager from \"@/components/diy/diy_pager\";\n\timport mixin from \"@/mixins/page.js\";\n\n\texport default {\n\t\tmixins: [mixin],\n\t\tcomponents: {\n\t\t\tdiy_pager,\n\t\t\tlist_book_information\n\t\t},\n\t\tdata() {\n\t\t\treturn {\n\t\t\t\turl_get_list: \"~/api/book_information/get_list?like=0\",\n\n\t\t\t\t// 查询条件\n\t\t\t\tquery: {\n\t\t\t\t\tkeyword: \"\",\n\t\t\t\t\tpage: 1,\n\t\t\t\t\tsize: 10,\n\t\t\t\t\t\t\t\t\t\"book_name\": \"\", // 图书名称\n\t\t\t\t\t\t\t\t\t\t\t\"book_category\": \"\", // 图书类别\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\"examine_state\": \"已通过\", // 审核状态\n\t\t\t\t},\n\n\t\t\t\t// 排序内容\n\t\t\t\tlist_sort: [{\n\t\t\t\t\t\tname: \"创建时间从高到低\",\n\t\t\t\t\t\tvalue: \"create_time desc\",\n\t\t\t\t\t},\n\t\t\t\t\t{\n\t\t\t\t\t\tname: \"创建时间从低到高\",\n\t\t\t\t\t\tvalue: \"create_time asc\",\n\t\t\t\t\t},\n\t\t\t\t\t{\n\t\t\t\t\t\tname: \"更新时间从高到低\",\n\t\t\t\t\t\tvalue: \"update_time desc\",\n\t\t\t\t\t},\n\t\t\t\t\t{\n\t\t\t\t\t\tname: \"更新时间从低到高\",\n\t\t\t\t\t\tvalue: \"update_time asc\",\n\t\t\t\t\t},\n\t\t\t\t\t\t\t{\n\t\t\t\t\t\tname: \"图书名称正序\",\n\t\t\t\t\t\tvalue: \"book_name asc\",\n\t\t\t\t\t},\n\t\t\t\t\t{\n\t\t\t\t\t\tname: \"图书名称倒序\",\n\t\t\t\t\t\tvalue: \"book_name desc\",\n\t\t\t\t\t},\n\t\t\t\t\t\t\t{\n\t\t\t\t\t\tname: \"图书类别正序\",\n\t\t\t\t\t\tvalue: \"book_category asc\",\n\t\t\t\t\t},\n\t\t\t\t\t{\n\t\t\t\t\t\tname: \"图书类别倒序\",\n\t\t\t\t\t\tvalue: \"book_category desc\",\n\t\t\t\t\t},\n\t\t\t\t\t\t\t\t\t\t\t\t],\n\n\t\t\t\t\t\t\t\t\t// 图书类别列表\n\t\t\t\t\"list_book_category\": [\"\"],\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\n\t\t\t\t\t\t\t\t\t\t\t\t\t\"list_type_book_category\": [],\n\t\t\t\t\"type_list\":{},\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t}\n\t\t},\n\t\tmethods: {\n      get_list_before(param) {\n        var oNowDate = this.dateFormat(\"yyyy-MM-dd hh:mm:ss\")+\"\";\n        var url_get_list = \"~/api/book_information/get_list?timing_start_time_max=\" + oNowDate +\n            \"&timing_end_time_min=\" + oNowDate + \"&like=0\";\n        this.url_get_list= url_get_list;\n      },\n\t\t\t/**\n\t\t\t * 筛选选择\n\t\t\t */\n\t\t\tfilter_set(o,key) {\n\t\t\t    if (o == \"全部\") {\n\t\t\t        this.query[key] = \"\";\n\t\t\t    } else {\n\t\t\t        this.query[key] = o;\n\t\t\t    }\n\t\t\t    this.search();\n\t\t\t},\n\n\t\t\t/**\n\t\t\t * 排序\n\t\t\t */\n\t\t\tset_sort(o) {\n\t\t\t    this.query.orderby = o.value;\n\t\t\t    this.search();\n\t\t\t},\n\n\t\t\t\t\t\t\t\t\t\t\t\tasync get_list_type_book_category() {\n\t\t\t\tvar json = await this.$get(\"~/api/book_information/get_list?groupby=book_category\");\n\t\t\t\tif (json.result) {\n\t\t\t\t\tthis.list_type_book_category = json.result.list;\n\t\t\t\t\tfor (let i = 0;i<this.list_type_book_category.length;i++){\n\t\t\t\t\t\tthis.get_book_category_type_list(this.list_type_book_category[i].book_category);\n\t\t\t\t\t}\n\t\t\t\t\tthis.query.book_category = this.list_type_book_category[0].book_category;\n\t\t\t\t\tthis.get_list();\n\t\t\t\t} else if (json.error) {\n\t\t\t\t\tconsole.log(json.error);\n\t\t\t\t}\n\t\t\t},\n\n\t\t\tget_book_category_type_list(type){\n\t\t\t\tlet _this = this;\n\t\t\t\tthis.$get(\"~/api/book_information/get_list?\", {\"book_category\":type}, (json) => {\n\t\t\t\t\tif (json.result) {\n\t\t\t\t\t\t_this.$set(_this.type_list,type, json.result.list);\n\t\t\t\t\t}\n\t\t\t\t});\n\t\t\t},\n\t\t\ttab_select(v){\n\t\t\t\tthis.query.book_category = v;\n\t\t\t\tthis.get_list();\n\t\t\t},\n\t\t\tsearch_book_information(){\n\t\t\t\tlet _this = this;\n\t\t\t\tthis.$get(\"~/api/book_information/get_list?like=0\", _this.query, (json) => {\n\t\t\t\t\tif (json.result) {\n\t\t\t\t\t\t_this.$set(_this.type_list,_this.query.book_category, json.result.list);\n\t\t\t\t\t\t_this.get_list();\n\t\t\t\t\t}\n\t\t\t\t});\n\t\t\t},\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\n\t\t\t\t\t\t/**\n\t\t\t * 获取图书类别列表\n\t\t\t */\n\t\t\tasync get_list_book_category() {\n\t\t\t\tvar json = await this.$get(\"~/api/book_classification/get_list?\");\n\t\t\t\tif (json.result) {\n\t\t\t\t\tthis.list_book_category = json.result.list;\n\t\t\t\t} else if (json.error) {\n\t\t\t\t\tconsole.log(json.error);\n\t\t\t\t}\n\t\t\t},\n\t\t\t\t\t\t\t\t\n\t\t\t/**\n\t\t\t * 筛选\n\t\t\t */\n\t\t\t\t\t\t\t\t\t\t\t\t\tfilter_book_category(o) {\n\t\t\t\tif (o == \"全部\") {\n\t\t\t\t\tthis.query[\"book_category\"] = \"\";\n\t\t\t\t} else {\n\t\t\t\t\tthis.query[\"book_category\"] = o;\n\t\t\t\t}\n\t\t\t\tthis.search();\n\t\t\t},\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t/**\n\t\t\t * 重置\n\t\t\t */\n\t\t\treset() {\n\t\t\t\t\t\t\t\tthis.query.book_name = \"\"\n\t\t\t\t\t\t\t\t\t\tthis.query.book_category = \"\"\n\t\t\t\t\t\t\t\t\t\t\t\t\t\tthis.search();\n\t\t\t},\n\n\t\t\t// 返回条数\n\t\t\ttoSize(i){\n\t\t\t\tthis.query.size = i;\n\t\t\t\tthis.first();\n\t\t\t},\n\n\t\t\t// 返回页数\n\t\t\ttoPage(i){\n\t\t\t\tthis.query.page = i;\n\t\t\t\tthis.first();\n\t\t\t},\n\n      goToPage(v){\n        this.query.page = v;\n        this.goToNew(v)\n      },\n\n\t\t\tdateFormat(fmt) {\n\t\t\t\tvar myDate = new Date();\n\t\t\t\tvar o = {\n\t\t\t\t\t\"M+\": myDate.getMonth() + 1, // 月份\n\t\t\t\t\t\"d+\": myDate.getDate(), // 日\n\t\t\t\t\t\"h+\": myDate.getHours(), // 小时\n\t\t\t\t\t\"m+\": myDate.getMinutes(), // 分\n\t\t\t\t\t\"s+\": myDate.getSeconds(), // 秒\n\t\t\t\t\t\"q+\": Math.floor((myDate.getMonth() + 3) / 3), // 季度\n\t\t\t\t\t\"S\": myDate.getMilliseconds() // 毫秒\n\t\t\t\t};\n\t\t\t\tif (/(y+)/.test(fmt))\n\t\t\t\t\tfmt = fmt.replace(RegExp.$1, (myDate.getFullYear() + \"\").substr(4 - RegExp.$1.length));\n\t\t\t\tfor (var k in o)\n\t\t\t\t\tif (new RegExp(\"(\" + k + \")\").test(fmt)) fmt = fmt.replace(RegExp.$1, (RegExp.$1.length == 1) ? (o[k]) : ((\"00\" + o[k]).substr((\"\" + o[k]).length)));\n\t\t\t\t\t\treturn fmt;\n\t\t\t},\n\t\t},\n\t\tcomputed: {\n\t\t},\n\t\tcreated() {\n\t\t\t\t\t\t\t\t/**\n\t\t\t * 获取图书类别列表\n\t\t\t */\n\t\t\tthis.get_list_book_category();\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\tthis.get_list_type_book_category();\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t}\n\t}\n</script>\n\n<style>\n</style>\n"]}]}