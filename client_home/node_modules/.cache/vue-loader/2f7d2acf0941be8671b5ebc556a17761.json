;AACA;AACA;AACA","file":"list.vue","sourceRoot":"src/views/website_products","sourcesContent":["<template>\n\t<div class=\"diy_list page_website_products\" id=\"website_products_list\">\n\t\t<div class=\"warp\">\n\t\t\t<div class=\"container diy_list_container\">\n\t\t\t\t<div class=\"diy_list_title\">\n\t\t\t\t\t<div class=\"col\">\n\t\t\t\t\t\t<span class=\"title\">网站商品列表</span>\n\t\t\t\t\t</div>\n\t\t\t\t</div>\n\t\t\t\t<div class=\"row diy_list_search\">\n\t\t\t\t\t<div class=\"col\">\n\t\t\t\t\t\t<!-- 搜索栏 -->\n\t\t\t\t\t\t<div class=\"view\">\n\t\t\t\t\t\t\t<span class=\"diy_list_search_title\">关键字搜索：</span>\n\t\t\t\t\t\t\t\t\t<b-form-input size=\"sm\" class=\"mr-sm-2\" placeholder=\"图书名称搜索\" v-model=\"query['book_name']\" />\n\t\t\t\t\t\t\t\t\t<b-form-input size=\"sm\" class=\"mr-sm-2\" placeholder=\"图书类别搜索\" v-model=\"query['book_category']\" />\n\t\t\t\t\t\t\t\t\t\t\t<b-form-input size=\"sm\" class=\"mr-sm-2\" placeholder=\"作者搜索\" v-model=\"query['author']\" />\n\t\t\t\t\t\t\t\t\t\t\t\t<b-button size=\"sm\" @click=\"search()\" >\n\t\t\t\t\t\t\t\t<b-icon icon=\"search\"/>\n\t\t\t\t\t\t\t</b-button>\n\t\t\t\t\t\t</div>\n\t\t\t\t\t\t<!-- /搜索栏 -->\n\t\t\t\t\t</div>\n\t\t\t\t</div>\n\t\t\t\t<div class=\"diy_list_select_box\">\n\t\t\t\t\t<span class=\"diy_list_select_title\">下拉搜索：</span>\n\t\t\t\t\t\t<div class=\"diy_list_dropdown_box\">\n\t\t\t\t\t\t<div class=\"col\">\n\t\t\t\t\t\t\t<!-- 筛选 -->\n\t\t\t\t\t\t\t<div class=\"view\">\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t<b-dropdown text=\"图书类别\" variant=\"outline-dark\" left>\n\t\t\t\t\t\t\t\t\t<b-dropdown-item @click=\"filter_set('全部','book_category')\">全部</b-dropdown-item>\n\t\t\t\t\t\t\t\t\t\t<b-dropdown-item v-for=\"(o, i) in list_book_category\" :key=\"i\" @click=\"filter_set(o['book_category'],'book_category')\" >\n\t\t\t\t\t\t\t\t\t\t\t\t{{ o['book_category'] }}\n\t\t\t\t\t\t\t\t\t\t</b-dropdown-item>\n\t\t\t\t\t\t\t\t</b-dropdown>\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t</div>\n\t\t\t\t\t\t\t<!-- /筛选 -->\n\t\t\t\t\t\t</div>\n\t\t\t\t\t</div>\n\t\t\t\t\t<div class=\"diy_list_sort_box\">\n\t\t\t\t\t\t<div class=\"col\">\n\t\t\t\t\t\t\t<!-- 排序 -->\n\t\t\t\t\t\t\t<div class=\"view\">\n\t\t\t\t\t\t\t\t<b-dropdown text=\"排序\" variant=\"outline-dark\" left>\n\t\t\t\t\t\t\t\t\t\t<b-dropdown-item v-for=\"(o, i) in list_sort\" :key=\"i\" @click=\"set_sort(o)\" >\n\t\t\t\t\t\t\t\t\t\t\t\t{{ o.name }}\n\t\t\t\t\t\t\t\t\t\t</b-dropdown-item>\n\t\t\t\t\t\t\t\t</b-dropdown>\n\t\t\t\t\t\t\t</div>\n\t\t\t\t\t\t\t<!--/排序 -->\n\t\t\t\t\t\t</div>\n\t\t\t\t\t</div>\n\t\t\t\t</div>\n\n\t\t\t\t<div class=\"row diy_list_box\">\n\t\t\t\t\t<div class=\"col\">\n\t\t\t\t\t\t<!-- 列表 -->\n\t\t\t\t\t\t<list_website_products :list=\"list\" />\n\t\t\t\t\t\t<!-- /列表 -->\n\t\t\t\t\t</div>\n\t\t\t\t</div>\n\t\t\t\t<div class=\"row diy_list_page_box\">\n\t\t\t\t\t<div class=\"col overflow-auto flex_cc\">\n\t\t\t\t\t\t<!-- 分页器 -->\n<!--\t\t\t\t\t\t<diy_pager v-model=\"query['page']\" :size=\"query['size']\" :count=\"count\" v-on:toPage=\"toPage\" v-on:toSize=\"toSize\" ></diy_pager>-->\n            <b-pagination\n                v-model=\"query.page\"\n                :total-rows=\"count\"\n                :per-page=\"query.size\"\n                @change=\"goToPage\"\n            />\n\t\t\t\t\t\t<!-- /分页器 -->\n\t\t\t\t\t</div>\n\t\t\t\t</div>\n\t\t\t</div>\n\t\t</div>\n\t</div>\n</template>\n\n<script>\n\timport list_website_products from \"@/components/diy/list_website_products.vue\";\n\timport diy_pager from \"@/components/diy/diy_pager\";\n\timport mixin from \"@/mixins/page.js\";\n\n\texport default {\n\t\tmixins: [mixin],\n\t\tcomponents: {\n\t\t\tdiy_pager,\n\t\t\tlist_website_products\n\t\t},\n\t\tdata() {\n\t\t\treturn {\n\t\t\t\turl_get_list: \"~/api/website_products/get_list?like=0\",\n\n\t\t\t\t// 查询条件\n\t\t\t\tquery: {\n\t\t\t\t\tkeyword: \"\",\n\t\t\t\t\tpage: 1,\n\t\t\t\t\tsize: 10,\n\t\t\t\t\t\t\t\t\t\"book_name\": \"\", // 图书名称\n\t\t\t\t\t\t\t\t\t\t\t\"book_category\": \"\", // 图书类别\n\t\t\t\t\t\t\t\t\t\t\t\t\t\"author\": \"\", // 作者\n\t\t\t\t\t\t\t\t\t\t\t},\n\n\t\t\t\t// 排序内容\n\t\t\t\tlist_sort: [{\n\t\t\t\t\t\tname: \"创建时间从高到低\",\n\t\t\t\t\t\tvalue: \"create_time desc\",\n\t\t\t\t\t},\n\t\t\t\t\t{\n\t\t\t\t\t\tname: \"创建时间从低到高\",\n\t\t\t\t\t\tvalue: \"create_time asc\",\n\t\t\t\t\t},\n\t\t\t\t\t{\n\t\t\t\t\t\tname: \"更新时间从高到低\",\n\t\t\t\t\t\tvalue: \"update_time desc\",\n\t\t\t\t\t},\n\t\t\t\t\t{\n\t\t\t\t\t\tname: \"更新时间从低到高\",\n\t\t\t\t\t\tvalue: \"update_time asc\",\n\t\t\t\t\t},\n\t\t\t\t\t\t\t{\n\t\t\t\t\t\tname: \"图书名称正序\",\n\t\t\t\t\t\tvalue: \"book_name asc\",\n\t\t\t\t\t},\n\t\t\t\t\t{\n\t\t\t\t\t\tname: \"图书名称倒序\",\n\t\t\t\t\t\tvalue: \"book_name desc\",\n\t\t\t\t\t},\n\t\t\t\t\t\t\t{\n\t\t\t\t\t\tname: \"图书类别正序\",\n\t\t\t\t\t\tvalue: \"book_category asc\",\n\t\t\t\t\t},\n\t\t\t\t\t{\n\t\t\t\t\t\tname: \"图书类别倒序\",\n\t\t\t\t\t\tvalue: \"book_category desc\",\n\t\t\t\t\t},\n\t\t\t\t\t\t\t\t\t{\n\t\t\t\t\t\tname: \"作者正序\",\n\t\t\t\t\t\tvalue: \"author asc\",\n\t\t\t\t\t},\n\t\t\t\t\t{\n\t\t\t\t\t\tname: \"作者倒序\",\n\t\t\t\t\t\tvalue: \"author desc\",\n\t\t\t\t\t},\n\t\t\t\t\t\t\t\t\t],\n\n\t\t\t\t\t\t\t\t\t// 图书类别列表\n\t\t\t\t\"list_book_category\": [\"\"],\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\n\t\t\t}\n\t\t},\n\t\tmethods: {\n      get_list_before(param) {\n      },\n\t\t\t/**\n\t\t\t * 筛选选择\n\t\t\t */\n\t\t\tfilter_set(o,key) {\n\t\t\t    if (o == \"全部\") {\n\t\t\t        this.query[key] = \"\";\n\t\t\t    } else {\n\t\t\t        this.query[key] = o;\n\t\t\t    }\n\t\t\t    this.search();\n\t\t\t},\n\n\t\t\t/**\n\t\t\t * 排序\n\t\t\t */\n\t\t\tset_sort(o) {\n\t\t\t    this.query.orderby = o.value;\n\t\t\t    this.search();\n\t\t\t},\n\n\n\t\t\t\t\t\t/**\n\t\t\t * 获取图书类别列表\n\t\t\t */\n\t\t\tasync get_list_book_category() {\n\t\t\t\tvar json = await this.$get(\"~/api/book_classification/get_list?\");\n\t\t\t\tif (json.result) {\n\t\t\t\t\tthis.list_book_category = json.result.list;\n\t\t\t\t} else if (json.error) {\n\t\t\t\t\tconsole.log(json.error);\n\t\t\t\t}\n\t\t\t},\n\t\t\t\t\t\t\t\t\n\t\t\t/**\n\t\t\t * 筛选\n\t\t\t */\n\t\t\t\t\t\t\t\t\t\t\t\t\tfilter_book_category(o) {\n\t\t\t\tif (o == \"全部\") {\n\t\t\t\t\tthis.query[\"book_category\"] = \"\";\n\t\t\t\t} else {\n\t\t\t\t\tthis.query[\"book_category\"] = o;\n\t\t\t\t}\n\t\t\t\tthis.search();\n\t\t\t},\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t/**\n\t\t\t * 重置\n\t\t\t */\n\t\t\treset() {\n\t\t\t\t\t\t\t\tthis.query.book_name = \"\"\n\t\t\t\t\t\t\t\t\t\tthis.query.book_category = \"\"\n\t\t\t\t\t\t\t\t\t\t\t\tthis.query.author = \"\"\n\t\t\t\t\t\t\t\t\t\t\tthis.search();\n\t\t\t},\n\n\t\t\t// 返回条数\n\t\t\ttoSize(i){\n\t\t\t\tthis.query.size = i;\n\t\t\t\tthis.first();\n\t\t\t},\n\n\t\t\t// 返回页数\n\t\t\ttoPage(i){\n\t\t\t\tthis.query.page = i;\n\t\t\t\tthis.first();\n\t\t\t},\n\n      goToPage(v){\n        this.query.page = v;\n        this.goToNew(v)\n      },\n\n\t\t},\n\t\tcomputed: {\n\t\t},\n\t\tcreated() {\n\t\t\t\t\t\t\t\t/**\n\t\t\t * 获取图书类别列表\n\t\t\t */\n\t\t\tthis.get_list_book_category();\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t}\n\t}\n</script>\n\n<style>\n</style>\n"]}]}