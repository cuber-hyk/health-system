t\t                var list_notice = json.result.list;\n\t\t\t                this.list_notice = list_notice;\n\t\t\t            }\n\t\t\t        }\n\t\t\t    );\n\t\t\t},\n\t\t\tto_details(key,o,id) {\n\t\t\t\tif(!id){\n\t\t\t\t\tid = key + \"_id\";\n\t\t\t\t}\n\t\t\t    this.$router.push('/' + key + '/details?' + id + '=' + o[id]);\n\t\t\t},\n\t\t\tasync get_list_type_commodity_type() {\n\t\t\t\tvar json = await this.$get(\"~/api/custom_goods/get_list?groupby=commodity_type\");\n\t\t\t\tif (json.result) {\n\t\t\t\t\tthis.list_type_commodity_type = json.result.list;\n\t\t\t\t\tconsole.log(this.list_type_commodity_type);\n\t\t\t\t\tthis.query.commodity_type = this.list_type_commodity_type[0].commodity_type;\n\t\t\t\t\tthis.get_list();\n\t\t\t\t} else if (json.error) {\n\t\t\t\t\tconsole.log(json.error);\n\t\t\t\t}\n\t\t\t}\n\t\t},\n\t\tmounted() {\n\t\t\t\t\t\tthis.get_food_information();\n\t\t\t\t\t\tthis.get_recipe_information();\n\t\t\t\tthis.get_menu();\n\t\t\tthis.get_slides();\n\t\t\tthis.get_article();\n\t\t\tthis.get_link();\n\t\t\tthis.get_notice();\n\t\t\tthis.get_ad();\n\t\t},\n\t\tcomputed: {\n\t\t\t\t\t    list_table_recipe_information() {\n\t\t        var list = this.list_food_information;\n\t\t        var list_table = [];\n\t\t        for (let i = 0; i < list.length; i++) {\n\t\t            list_table[i] = {};\n\t\t            this.vm_arr_food_information.map((o) => {\n\t\t                // 第二个中括号是对象的属性\n\t\t                list_table[i][o] = list[i][o] || \"\";\n\t\t            });\n\t\t        }\n\t\t        return list_table;\n\t\t    },\n\t\t\t\t\t    list_table_recipe_information() {\n\t\t        var list = this.list_recipe_information;\n\t\t        var list_table = [];\n\t\t        for (let i = 0; i < list.length; i++) {\n\t\t            list_table[i] = {};\n\t\t            this.vm_arr_recipe_information.map((o) => {\n\t\t                // 第二个中括号是对象的属性\n\t\t                list_table[i][o] = list[i][o] || \"\";\n\t\t            });\n\t\t      