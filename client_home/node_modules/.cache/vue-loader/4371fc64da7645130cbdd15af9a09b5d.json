"row\">\n          <div class=\"col\">\n            <div class=\"card_ad\">\n              <!-- 顶部 -->\n              <list_ad\n                :list=\"list_ad\"\n                v-if=\"$check_action('/ad/list', 'get')\"\n                location=\"顶部广告\"\n              />\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n\n    \n    <div class=\"warp\">\n      <div class=\"container\">\n        <div class=\"row\">\n          <div class=\"col\">\n            <div class=\"card_ad\">\n              <!-- 中部 -->\n              <list_ad\n                :list=\"list_ad\"\n                v-if=\"$check_action('/ad/list', 'get')\"\n                location=\"中部广告\"\n              />\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n\n        <div class=\"warp\">\n      <!-- 容器 -->\n      <div class=\"container\">\n        <div class=\"row\">\n          <div class=\"col-12\">\n            <!-- 标题栏组件 -->\n            <div class=\"article_recommend\">\n              <div class=\"title\">\n                <span> 健康百科 </span>\n              </div>\n              <div class=\"more_box\">\n                <router-link to=\"/article/list\" class=\"more\">\n                  <span>More</span>\n                </router-link>\n              </div>\n            </div>\n            <!-- 健康百科列表组件 -->\n            <list_article v-if=\"show_list_article\" :list=\"list_article\" />\n            <div class=\"overflow-auto\" v-else>\n              <table\n                id=\"list_article\"\n                role=\"table\"\n                aria-busy=\"false\"\n                :aria-colcount=\"fields_article.length\"\n                class=\"table b-table table-striped table-hover\"\n              >\n                <thead>\n                  <tr>\n                    <th v-for=\"(o, i) in fields_article\" :key=\"i\">\n                      {{ o.label }}\n                    </th>\n                  </tr>\n                </thead>\n                <tbody>\n                  <tr v-for=\"(o, i) in list_table_article\" :key=\"i\">\n                    <td\n                      v-for=\"(oj, n) in fields_goods\"\n                      @click=\"to_details('goods', o)\"\n                    >\n                      <img\n                        v-if=\"oj.type && oj.type == '图片'\"\n                        :src=\"$fullUrl(o[oj.key])\"\n                        alt=\"\"\n                        v-default-img=\"'/img/default.png'\"\n                      />\n                      <span v-else>{{ o[oj.key] }}</span>\n                    </td>\n                  </tr>\n                </tbody>\n              </table>\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n    \n    <div class=\"warp\">\n      <div class=\"container\">\n        <div class=\"row\">\n          <div class=\"col\">\n            <div class=\"card_ad\">\n              <!-- 底部 -->\n              <list_ad\n                :list=\"list_ad\"\n                v-if=\"$check_action('/ad/list', 'get')\"\n                location=\"底部广告\"\n              />\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n\n             <!-- 推荐食物信息模块(开始) -->\n    <div class=\"warp\" v-if=\"$check_action('/food_information/list', 'get')\">\n      <!-- 容器 -->\n      <div class=\"container\">\n        <div class=\"row\">\n          <div class=\"col-12\">\n            <!-- 标题栏组件 -->\n            <div class=\"diy_recommend\">\n              <div class=\"title\">\n                <span> 食物信息推荐 </span>\n              </div>\n              <div class=\"more_box\">\n                <router-link to=\"/food_information/list\" class=\"more\">\n                  <span>More</span>\n                </router-link>\n              </div>\n            </div>\n            <!-- 食物信息推荐列表组件 -->\n            <list_food_information v-if=\"show_list_food_information\" :list=\"list_food_information\" />\n            <div class=\"overflow-auto\" v-else>\n              <table\n                id=\"list_diy\"\n                role=\"table\"\n                aria-busy=\"false\"\n                :aria-colcount=\"fields_food_information.length\"\n                class=\"table b-table table-striped table-hover\"\n              >\n                <thead>\n                  <tr>\n                    <th v-for=\"(o,i) in fields_food_information\" :key=\"i\">\n                      {{ o.label }}\n                    </th>\n                  </tr>\n                </thead>\n                <tbody>\n                  <tr\n                    v-for=\"(o, i) in list_table_food_information\"\n                    :key=\"i\"\n                    @click=\"to_details('food_information', o, 'food_information_id')\"\n                  >\n                    <td v-for=\"(oj,n) in fields_food_information\">\n                      <img\n                        v-if=\"oj.type && oj.type == '图片'\"\n                        :src=\"$fullUrl(o[oj.key])\"\n                        alt=\"\"\n                        v-default-img=\"'/img/default.png'\"\n                      />\n                      <span v-else-if=\"oj.type && oj.type == 'UID'\">\n                        <span style=\"display: none\">\n                          {{ get_user_by_user_id(o, oj, oj.key) }}\n                        </span>\n                        {{ oj.value }}\n                      </span>\n                      <span v-else>{{ o[oj.key] }}</span>\n                    </td>\n                  </tr>\n                </tbody>\n              </table>\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n    <!-- 推荐食物信息模块(结束) -->\n            <!-- 推荐食谱信息模块(开始) -->\n    <div class=\"warp\" v-if=\"$check_action('/recipe_information/list', 'get')\">\n      <!-- 容器 -->\n      <div class=\"container\">\n        <div class=\"row\">\n          <div class=\"col-12\">\n            <!-- 标题栏组件 -->\n            <div class=\"diy_recommend\">\n              <div class=\"title\">\n                <span> 食谱信息推荐 </span>\n              </div>\n              <div class=\"more_box\">\n                <router-link to=\"/recipe_information/list\" class=\"more\">\n                  <span>More</span>\n                </router-link>\n              </div>\n            </div>\n            <!-- 食谱信息推荐列表组件 -->\n            <list_recipe_information v-if=\"show_list_recipe_information\" :list=\"list_recipe_information\" />\n            <div class=\"overflow-auto\" v-else>\n              <table\n                id=\"list_diy\"\n                role=\"table\"\n                aria-busy=\"false\"\n                :aria-colcount=\"fields_recipe_information.length\"\n                class=\"table b-table table-striped table-hover\"\n              >\n                <thead>\n                  <tr>\n                    <th v-for=\"(o,i) in fields_recipe_information\" :key=\"i\">\n                      {{ o.label }}\n                    </th>\n                  </tr>\n                </thead>\n                <tbody>\n                  <tr\n                    v-for=\"(o, i) in list_table_recipe_information\"\n                    :key=\"i\"\n                    @click=\"to_details('recipe_information', o, 'recipe_information_id')\"\n                  >\n                    <td v-for=\"(oj,n) in fields_recipe_information\">\n                      <img\n                        v-if=\"oj.type && oj.type == '图片'\"\n                        :src=\"$fullUrl(o[oj.key])\"\n                        alt=\"\"\n                        v-default-img=\"'/img/default.png'\"\n                      />\n                      <span v-else-if=\"oj.type && oj.type == 'UID'\">\n                        <span style=\"display: none\">\n                          {{ get_user_by_user_id(o, oj, oj.key) }}\n                        </span>\n                        {{ oj.value }}\n                      </span>\n                      <span v-else>{{ o[oj.key] }}</span>\n                    </td>\n                  </tr>\n                </tbody>\n              </table>\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n    <!-- 推荐食谱信息模块(结束) -->\n     \n    <div class=\"warp\">\n      <div class=\"container\">\n        <div class=\"row\">\n          <div class=\"col\">\n            <div class=\"card_link\">\n              <!-- 底部友情链接 -->\n              <list_link\n                v-if=\"$check_action('/link/list', 'get')\"\n                :list=\"list_link\"\n              />\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\n\timport mixin from \"@/mixins/page.js\";\n\t\t\t\timport list_food_information from \"@/components/diy/list_food_information.vue\";\n\t\t\t\timport list_recipe_information from \"@/components/diy/list_recipe_information.vue\";\n\t\timport bar_title from \"@/components/diy/bar_title.vue\";\n\timport list_article from \"@/components/diy/list_article.vue\";\n\timport swiper_img from \"@/components/diy/swiper_img.vue\";\n\timport list_ad from \"@/components/diy/list_ad.vue\";\n\timport list_link from \"@/components/diy/list_link.vue\";\n\timport swiper_notice from \"@/components/diy/swiper_notice.vue\";\n\n\texport default {\n\t\tmixins: [mixin],\n\t\tcomponents: {\n\t\t\t\t\t\tlist_food_information,\n\t\t\t\t\t\tlist_recipe_information,\n\t\t\t\tbar_title,\n\t\t\tlist_article,\n\t\t\tswiper_img,\n\t\t\tswiper_notice,\n\t\t\tlist_ad,\n\t\t\tlist_link,\n\t\t},\n\t\tdata() {\n\t\t\treturn {\n\t\t\t\tisSmall: false,\n\t\t\t\tsendValue: \"\",\n\t\t\t\tchatList:[],\n\t\t\t\tshowChat: false,\n\t\t\t\tisAdmin: false,\n\t\t\t\ttoken:\"\",\n        \"list_type_commodity_type\": [],\n\t\t\t\t// 文章模型数组\n\t\t\t\tlist_article: [],\n\t\t\t\tvm_arr_article: [\n\t\t\t\t    \"img\",\n\t\t\t\t    \"title\",\n\t\t\t\t    \"description\",\n\t\t\t\t    \"hits\",\n\t\t\t\t    \"type\",\n\t\t\t\t    \"tag\",\n\t\t\t\t    