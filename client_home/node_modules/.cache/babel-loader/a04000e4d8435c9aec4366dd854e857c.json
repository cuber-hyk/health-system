CA;AACA;AACA;IACAC;MACA;MAEA;QACAC;QACAC;QACAC;QACAC;MACA;MAEA;;MAEA;MACA;QACA;QACA;UACA;YACAC;YACA;YACA,WACA,oDACAA,kCACA;cACAC;YACA,GACAC;cACA;gBACAC;cACA;gBACAA;cACA;YACA,EACA;YACA;YACA;UACA;YACA;YACAA;UACA;QACA;MACA;QACA;QACA;UACA;YACAH;YACA;YACA,WACA,oDACAA,kCACA;cACAC;YACA,GACAC;cACA;gBACAC;cACA;gBACAA;cACA;YACA,EACA;YACA;YACA;UACA;YACA;YACAA;UACA;QACA;MACA;IACA;IACA;AACA;AACA;IACAC;MACA;MACA,UACA,uBACA;QACAR;QACAC;QACAC;QACAC;MACA,GACAG;QACA;UACAC;UACA;UACAA;QACA;UACA;UACAA;QACA;MACA,EACA;IACA;IAEA;AACA;AACA;IACAE;MACA;MAEA;QACAT;QACAC;QACAC;QACAC;MACA;;MAEA;MACA;QACA;QACA;UACA;YACA;YACA;UACA;YACA;YACAI;UACA;QACA;MACA;QACA;QACAG;QACAA;QACA;UACA;YACA;YACA;UACA;YACA;YACAH;UACA;QACA;MACA;IACA;IACAI;MACA;MACA;MACA,UACA,wBACA;QACAR;QACAH;QACAC;QACAC;MACA,GACAI;QACA;MACA,EACA;IACA;EACA;EACAM;IACAC;MACA;MACA;IACA;EACA;EACAC;IACA/B;MACAgC;MACAC;QACA;MACA;IACA;EACA;AACA","names":["mixins","components","props","obj","type","default","data","praiseLen","imgList","title","name","itemList","richList","computed","methods","get_user_info","nickname","username","ret","change_praise","source_table","source_field","source_id","user_id","_this","praise_len","res","console","get_praise","change_collect","query","check_collect","created","setTimeout","watch","deep","handler"],"sourceRoot":"src/components/diy","sources":["div_food_information.vue"],"sourcesContent":["<template>\n  <div class=\"diy_details_box diy_div_food_information\">\n    <div class=\"warp\">\n      <div class=\"container\">\n        <div class=\"details_title\">详情</div>\n        <div class=\"row\">\n          <div class=\"row_top_wrap\">\n            <!-- 图片 -->\n            <div class=\"row_1\" v-if=\"imgList.length\">\n              <div\n                class=\"diy_img\"\n                v-for=\"(item, index) in imgList\"\n                :key=\"item + index\"\n              >\n                <img :src=\"$fullUrl(obj[item.name])\" />\n              </div>\n            </div>\n            <!-- <div class=\"row_6\" v-if=\"imgList.length\" style=\"text-align: center;\">\n\t\t\t\t\t\t\t\t\t<div class=\"diy_name\" v-for=\"(item,index) in itemList\" :key=\"item+index\">\n\t\t\t\t\t\t\t\t\t<span>{{item.title}}</span><span>:&nbsp;&nbsp;&nbsp;</span><span>{{obj[item.name]}}</span>\n\t\t\t\t\t\t\t\t\t</div>\n\t\t\t\t\t\t\t</div>\t\n\t\t\t\t\t\t\t<div class=\"row_6\" v-if=\"imgList.length\" style=\"text-align: right;\">  \n    \t\t\t\t\t\t\t\t<div class=\"diy_name\" v-for=\"(item, index) in itemList\" :key=\"item + index\">  \n       \t\t\t\t\t\t\t\t\t<span class=\"field-label\">{{ item.title }}</span>  \n        \t\t\t\t\t\t\t\t<span class=\"field-separator\">:&nbsp;&nbsp;&nbsp;</span>  \n        \t\t\t\t\t\t\t\t<span class=\"field-value\">{{ obj[item.name] }}</span>  \n    \t\t\t\t\t\t\t\t</div>  \n\t\t\t\t\t\t\t</div>\t-->\n            <!-- 内容 -->\n            <div class=\"row_2\" :class=\"{ flex_row2: !imgList.length }\">\n              <div\n                :class=\"{ flex_row2_item: !imgList.length }\"\n                v-for=\"(item, index) in itemList\"\n                :key=\"item + index\"\n                v-show=\"$check_field('get', item.name)\"\n              >\n                <div class=\"view\">\n                  <div class=\"diy_title\">\n                    <span>{{ item.title }}</span>\n                  </div>\n\n                  <div\n                    class=\"diy_field diy_uid\"\n                    :class=\"{ flex_row2_item_text: imgList.length }\"\n                    v-if=\"item.type == 'UID'\"\n                  >\n                    <span>\n                      {{ get_user_info(item.name, obj[item.name]) }}\n                    </span>\n                  </div>\n                  <div\n                    class=\"diy_field diy_video\"\n                    :class=\"{ flex_row2_item_text: imgList.length }\"\n                    v-else-if=\"item.type == '视频'\"\n                  >\n                    <router-link\n                      :to=\"'/media/video?filename=' + $fullUrl(obj[item.name])\"\n                      v-if=\"obj[item.name]\"\n                    >\n                      <span> 查看视频 </span>\n                    </router-link>\n                  </div>\n                  <div\n                    class=\"diy_field diy_music\"\n                    :class=\"{ flex_row2_item_text: imgList.length }\"\n                    v-else-if=\"item.type == '音频'\"\n                  >\n                    <audio\n                      v-if=\"obj[item.name]\"\n                      style=\"text-align: left\"\n                      :src=\"$fullUrl(obj[item.name])\"\n                      controls\n                    ></audio>\n                  </div>\n                  <div\n                    class=\"diy_field diy_music\"\n                    :class=\"{ flex_row2_item_text: imgList.length }\"\n                    v-else-if=\"item.type == '文件'\"\n                  >\n                    <a\n                      :href=\"$fullUrl(obj[item.name])\"\n                      target=\"_blank\"\n                      style=\"color: rgb(64, 158, 255)\"\n                      >点击下载</a\n                    >\n                  </div>\n                  <div\n                    class=\"diy_field diy_date\"\n                    :class=\"{ flex_row2_item_text: imgList.length }\"\n                    v-else-if=\"item.type == '日期'\"\n                  >\n                    <span>\n                      {{ $toTime(obj[item.name], \"yyyy-MM-dd\") }}\n                    </span>\n                  </div>\n                  <div\n                    class=\"diy_field diy_time\"\n                    :class=\"{ flex_row2_item_text: imgList.length }\"\n                    v-else-if=\"item.type == '时间'\"\n                  >\n                    <span>\n                      {{ $toTime(obj[item.name], \"hh:mm:ss\") }}\n                    </span>\n                  </div>\n                  <div\n                    class=\"diy_field diy_datetime\"\n                    :class=\"{ flex_row2_item_text: imgList.length }\"\n                    v-else-if=\"item.type == '日长'\"\n                  >\n                    <span>\n                      {{ $toTime(obj[item.name], \"yyyy-MM-dd hh:mm:ss\") }}\n                    </span>\n                  </div>\n                  <div\n                    class=\"diy_field diy_phone\"\n                    :class=\"{ flex_row2_item_text: imgList.length }\"\n                    v-else-if=\"item.type == '电话' || item.type == '手机'\"\n                  >\n                    <span>\n                      {{ obj[item.name] }}\n                    </span>\n                  </div>\n                  <div\n                    class=\"diy_field diy_number\"\n                    :class=\"{ flex_row2_item_text: imgList.length }\"\n                    v-else-if=\"item.type == '数字'\"\n                  >\n                    <span>\n                      {{ obj[item.name] }}\n                    </span>\n                  </div>\n                  <div\n                    class=\"diy_field diy_url\"\n                    :class=\"{ flex_row2_item_text: imgList.length }\"\n                    v-else-if=\"item.type == '网址'\"\n                  >\n                    <div>\n                      <a\n                        :href=\"obj[item.name]\"\n                        target=\"_blank\"\n                        style=\"color: rgb(64, 158, 255)\"\n                        >{{ obj[item.name] }}</a\n                      >\n                    </div>\n                  </div>\n                  <div\n                    class=\"diy_field diy_desc\"\n                    :class=\"{ flex_row2_item_text: imgList.length }\"\n                    v-else-if=\"item.type == '多文本'\"\n                  >\n                    <div>\n                      {{ obj[item.name] }}\n                    </div>\n                  </div>\n                  <!-- <div class=\"diy_field diy_html\"  v-else-if=\"item.type == '编辑'\" v-html=\"obj[item.name]\" >\n\n\t\t\t\t\t\t\t\t</div> -->\n                  <div\n                    class=\"diy_field diy_text\"\n                    :class=\"{ flex_row2_item_text: imgList.length }\"\n                    v-else\n                  >\n                    <span>\n                      {{ obj[item.name] }}\n                    </span>\n                  </div>\n                </div>\n              </div>\n            </div>\n          </div>\n          <div class=\"view praise_wrap\">\n            <span class=\"diy_title\">点赞数：</span>\n            <span class=\"diy_field diy_text\">\n              {{ praiseLen }}\n            </span>\n          </div>\n          <div class=\"details_btn_wrap\">\n            <button\n              class=\"btn btn-primary details_btn\"\n              v-if=\"$check_action('/food_information/details', 'set')\"\n              @click=\"change_praise()\"\n            >\n              点赞\n            </button>\n            <button\n              class=\"btn btn-primary details_btn\"\n              v-if=\"$check_action('/food_information/details', 'set')\"\n              @click=\"change_collect()\"\n            >\n              收藏\n            </button>\n          </div>\n          <!-- 富文本 -->\n          <div\n            class=\"rich_text\"\n            v-for=\"(item, index) in richList\"\n            :key=\"item + index\"\n            v-show=\"$check_field('get', item.name)\"\n          >\n            <div class=\"view\">\n              <div class=\"diy_title\">\n                <span v-text=\"item.title\"></span>\n              </div>\n              <div class=\"diy_html\" v-html=\"obj[item.name]\"></div>\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport mixin from \"@/mixins/component.js\";\nimport mixinPage from \"@/mixins/page.js\";\n\nexport default {\n  mixins: [mixin, mixinPage],\n  components: {},\n  props: {\n    obj: {\n      type: Object,\n      default: function () {\n        return {};\n      },\n    },\n  },\n  data() {\n    return {\n      praiseLen: this.obj[\"praise_len\"],\n      imgList: [\n        {\n          title: \"封面\",\n          name: \"cover\",\n          type: \"图片\",\n        },\n      ],\n      itemList: [\n        {\n          title: \"食物名称\",\n          name: \"food_name\",\n          type: \"文本\",\n        },\n        {\n          title: \"食物分类\",\n          name: \"food_classification\",\n          type: \"下寻\",\n        },\n        {\n          title: \"食用禁忌\",\n          name: \"taboo\",\n          type: \"文本\",\n        },\n        {\n          title: \"能量\",\n          name: \"energy\",\n          type: \"文本\",\n        },\n        {\n          title: \"适用人群\",\n          name: \"intended_for\",\n          type: \"文本\",\n        },\n      ],\n      richList: [\n        {\n          title: \"食物介绍\",\n          name: \"food_introduction\",\n          type: \"编辑\",\n        },\n      ],\n    };\n  },\n  computed: {\n    praiseLen() {\n      return this.obj.praise_len || 0; // 如果 obj.praise_len 不存在，返回 0 或其他默认值\n    },\n  },\n  methods: {\n    get_user_info(name, id) {\n      var obj = {\n        nickname: name, // 使用传入的name作为nickname\n        username: \"user_\" + id, // 使用传入的id构造一个username\n      };\n      var ret = \"\";\n      if (obj) {\n        ret = obj.nickname + \"-\" + obj.username;\n\n        // if(obj.nickname){\n        // \tret = obj.nickname;\n        // }\n        // else{\n        // \tret = obj.username;\n        // }\n      }\n      return ret;\n    },\n    /**\n     * 改变点赞数\n     */\n    change_praise(obj) {\n      var user_id = this.user.user_id;\n\n      var query = {\n        source_table: \"food_information\",\n        source_field: \"food_information_id\",\n        source_id: this.obj[\"food_information_id\"],\n        user_id,\n      };\n\n      var _this = this;\n\n      // 点赞状态\n      if (this.state_praise) {\n        this.state_praise = false;\n        this.$get(\"~/api/praise/del?\", query, (res) => {\n          if (res.result) {\n            _this.praiseLen = parseInt(_this.praiseLen) - 1;\n            var praise_len = _this.praiseLen;\n            this.$post(\n              \"~/api/food_information/set?food_information_id=\" +\n                _this.obj[\"food_information_id\"],\n              {\n                praise_len,\n              },\n              (res) => {\n                if (res.result) {\n                  console.log(\"添加点赞数状态：\", res.result);\n                } else if (res.error) {\n                  console.error(res.error);\n                }\n              }\n            );\n            // this.$toast(\"取消点赞\");\n            this.$message.success(\"取消点赞\");\n          } else if (res.error) {\n            this.$toast(res.error.message);\n            console.error(res.error);\n          }\n        });\n      } else {\n        this.state_praise = true;\n        this.$post(\"~/api/praise/add?\", query, (res) => {\n          if (res.result) {\n            _this.praiseLen = parseInt(_this.praiseLen) + 1;\n            var praise_len = _this.praiseLen;\n            this.$post(\n              \"~/api/food_information/set?food_information_id=\" +\n                _this.obj[\"food_information_id\"],\n              {\n                praise_len,\n              },\n              (res) => {\n                if (res.result) {\n                  console.log(\"添加点赞数状态：\", res.result);\n                } else if (res.error) {\n                  console.error(res.error);\n                }\n              }\n            );\n            // this.$toast(\"点赞成功\");\n            this.$message.success(\"点赞成功\");\n          } else if (res.error) {\n            this.$toast(res.error.message);\n            console.error(res.error);\n          }\n        });\n      }\n    },\n    /**\n     * 获取点赞\n     */\n    get_praise() {\n      var user_id = this.$store.state.user.user_id;\n      this.$get(\n        \"~/api/praise/count?\",\n        {\n          source_table: \"food_information\",\n          source_field: \"food_information_id\",\n          source_id: this.obj.food_information_id,\n          user_id,\n        },\n        (res) => {\n          if (res.result || res.result === 0) {\n            console.log(\"sadsfasf\" + JSON.stringify(res));\n            this.state_praise = res.result ? true : false;\n            console.log(\"点赞状态：\", this.state_praise);\n          } else if (res.error) {\n            this.$toast(res.error.message);\n            console.error(res.error);\n          }\n        }\n      );\n    },\n\n    /**\n     * 改变收藏状态\n     */\n    change_collect() {\n      var user_id = this.user.user_id;\n\n      var query = {\n        source_table: \"food_information\",\n        source_field: \"food_information_id\",\n        source_id: this.obj[\"food_information_id\"],\n        user_id,\n      };\n\n      // 收藏状态\n      if (this.state_collect) {\n        this.state_collect = false;\n        this.$get(\"~/api/collect/del?\", query, (res) => {\n          if (res.result) {\n            // this.$toast(\"取消收藏\");\n            this.$message.success(\"取消收藏\");\n          } else if (res.error) {\n            this.$toast(res.error.message);\n            console.error(res.error);\n          }\n        });\n      } else {\n        this.state_collect = true;\n        query.title = this.obj.food_name;\n        query.img = this.obj.cover;\n        this.$post(\"~/api/collect/add?\", query, (res) => {\n          if (res.result) {\n            // this.$toast(\"收藏成功\");\n            this.$message.success(\"收藏成功\");\n          } else if (res.error) {\n            this.$toast(res.error.message);\n            console.error(res.error);\n          }\n        });\n      }\n    },\n    check_collect() {\n      var user_id = this.$store.state.user.user_id;\n      var food_information_id = this.obj.food_information_id;\n      this.$get(\n        \"~/api/collect/count?\",\n        {\n          user_id,\n          source_table: \"food_information\",\n          source_field: \"food_information_id\",\n          source_id: food_information_id,\n        },\n        (res) => {\n          this.state_collect = res.result;\n        }\n      );\n    },\n  },\n  created() {\n    setTimeout(() => {\n      this.get_praise();\n      this.check_collect();\n    }, 1000);\n  },\n  watch: {\n    obj: {\n      deep: true,\n      handler() {\n        this.praiseLen = this.obj[\"praise_len\"];\n      },\n    },\n  },\n};\n</script>\n\n<style>\n.diy_details {\n  width: 1140px;\n  margin: 0 auto 50px;\n  background: #fff;\n  box-sizing: border-box;\n  padding: 30px;\n}\n.details_title {\n  width: 100%;\n  height: 50px;\n  line-height: 50px;\n  background: rgb(4, 126, 134);\n  font-size: 22px;\n  color: #fff;\n  margin-bottom: 40px;\n  padding-left: 15px;\n  overflow: hidden;\n}\n.row_top_wrap {\n  display: flex;\n  flex-direction: row;\n  margin-bottom: 45px;\n  min-height: 225px;\n}\n.row_top_wrap .row_1 {\n  width: 300px;\n  display: flex;\n  flex-direction: row;\n  margin-right: 10px;\n}\n.row_top_wrap .row_1 .diy_img,\n.row_top_wrap .row_1 .diy_img img {\n  width: 100%;\n  height: 100%;\n  border-radius: 15px;\n  margin-bottom: 4px;\n  box-shadow: 0 2px 16px 2px #ccc;\n}\n.diy_details_box .warp .container .row {\n  justify-content: start;\n}\n.row_top_wrap .row_6 {\n  width: 840px;\n  display: flex;\n  flex-direction: column;\n  align-items: flex-end;\n  gap: 10px; /* 添加间隔以分隔每个.diy_name块 */\n}\n\n.diy_name {\n  display: flex;\n  align-items: center;\n  padding: 10px; /* 内边距 */\n  border: 1px solid #ddd; /* 边框 */\n  border-radius: 5px; /* 圆角 */\n  background-color: #fff; /* 背景色 */\n  width: 80%; /* 宽度调整，根据需要修改 */\n}\n\n.field-label {\n  font-weight: bold; /* 加粗标签 */\n  margin-right: 10px; /* 右侧边距 */\n}\n\n.field-separator {\n  color: #999; /* 改变分隔符颜色 */\n}\n\n.field-value {\n  color: #333; /* 改变值颜色 */\n}\n/*\n.row_top_wrap .row_2{\n\twidth